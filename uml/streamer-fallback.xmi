      <UML:Package stereotype="folder" visibility="public" isSpecification="false" namespace="Logical View" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="ZFIAJN8HdhP9" name="overview">
       <UML:Namespace.ownedElement>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="WgMA7nuHYjyC" name="Listener">
         <UML:Namespace.ownedElement>
          <UML:Class visibility="public" isSpecification="false" namespace="WgMA7nuHYjyC" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="AlphgbCXQaAH" name="LiquidSoap configuration"/>
         </UML:Namespace.ownedElement>
        </UML:Class>
        <UML:Enumeration stereotype="enum" visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="R3mptdyUyMiT" comment="Says whether the current outage is to be acknowledged as planned,  unplanned or not acknowledged at all." name="situation">
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="R3mptdyUyMiT" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="WYTSuBw4EpKh" value="1" comment="Downtime is planned." name="planned downtime"/>
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="R3mptdyUyMiT" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="ZOjgGJfkp6Dj" value="2" comment="Downtime is not planned." name="unplanned downtime"/>
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="R3mptdyUyMiT" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="LBEuiUrHsav9" value="3" comment="No, we're not down, what are you talking about?" name="hide downtime"/>
        </UML:Enumeration>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="Ma82NIQvQRID" name="Schedule"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="Dgb1Zwr9I6NS" name="datetime"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="bH8T4frNjBJO" name="IceCast2"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="t6n5wyFVFLyI" name="LiquidSoap"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="spOdtwYWAqjn" name="IceCast2 configuration"/>
        <UML:Enumeration stereotype="enum" visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="ioOY0wIqWwRH" comment="Enum which tells what the listener shall hear if the streamer goes down." name="statuses">
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="ioOY0wIqWwRH" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="kqpGkEVi9JmB" value="1" comment="A constant loop of a jingle saying that there are technical problems. Depending on the current situation, the problem may be presented as being planned or not planned (not acknowledged is treated as not planned)." name="technical problems-jingle"/>
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="ioOY0wIqWwRH" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="8FE72k7mZw0H" value="2" comment="A collection of music from a set folder is played, with stinger playing between each track. The current situation decides which jingle to play in between." name="nightmusic"/>
         <UML:EnumerationLiteral visibility="public" isSpecification="false" namespace="ioOY0wIqWwRH" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="vI6sEu6M9JU9" value="3" comment="Transparent: make an attempt to cover up the lack of a proper stream by playing the same reruns that were scheduled to be played. The current situation is ignored. Shows with a name matching nightmusic_show_name will play nightmusic. Shows that look like live shows (no (R) in title and no rerun planned on the rerunner) will play the technical problems-jingle if live_shows_present is true, reruns of that show is run if not." name="reruns"/>
        </UML:Enumeration>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="dEKFeH5WLsbH" name="streamer-fallback"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="GEaXtZSNGmYN" name="Technician"/>
        <UML:Class visibility="public" isSpecification="false" namespace="ZFIAJN8HdhP9" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="NAWpxh3tj9Sv" name="core">
         <UML:Classifier.feature>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="WaPSz3FOF37d" type="ioOY0wIqWwRH" comment="What to do in case of downtime. The options are technical problems-jingle, nightmusic or transparent (that is, play reruns of whatever was supposed to be playing. When a rerun is scheduled and there is downtime, the listener might not even notice.)" name="current_status"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="rZtgf4PtD8LA" type="R3mptdyUyMiT" comment="The current situation of the downtime. Determines whether to acknowledge the downtime or not, and whether to call the downtime planned or not." name="current_situation"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="badmjzutR0xT" comment="File pointer to the LiquidSoap configuration file (or something along those lines…)" name="ls_config_fp"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="0swxs4Un690A" type="kagvPyRWnTT2" comment="True if technical problems should be played during live shows (that is, shows without (R) in the title and nothing planned in the rerunner), false if live shows should be treated as reruns (thus playing reruns if streamer goes down during the show). This affects what the listener hears if streamer goes down while the live show is on air." name="live_shows"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="Z82NpV8nN3kF" type="HJWkeOkEp1og" comment="Shows with this name in the schedule are assumed to consist of night music (that is, music from a set local directory populated by some other, seperate program). Thus, even if the status is &quot;reruns&quot;, night music is played in that spot." initialValue="Nattmusikk" name="nightmusic_show_name"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="7E2BM0LUgEwX" type="Ma82NIQvQRID" comment="List of schedules, one per day. Usually, a couple days of schedules are downloaded in advance, so that when looking at which files to remove locally, we do not remove a file we will need soon again anyways." name="schedule"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="YruPLLB1IfDP" type="kagvPyRWnTT2" comment="True if LiquidSoap needs to be restarted due to new config, False if not. Can be determined by looking at the script's modification datetime and the last time LiquidSoap was restarted." name="liquidsoap_needs_restart"/>
          <UML:Attribute visibility="private" isSpecification="false" xmi.id="RwLUEHj3Np5t" type="apXpa374VWMK" comment="Last time LiquidSoap was restarted." name="liquidsoap_last_restart_time"/>
          <UML:Operation visibility="public" isSpecification="false" isQuery="false" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="WtqF28eiGIEx" comment="Restart LiquidSoap, to apply any changes to the LiquidSoap configuration file." name="restart">
           <UML:BehavioralFeature.parameter>
            <UML:Parameter kind="return" xmi.id="UcmkvVkC7asg" type="kagvPyRWnTT2"/>
            <UML:Parameter visibility="private" isSpecification="false" xmi.id="ptUwNigyGJjz" type="HJWkeOkEp1og" value="0" comment="When to restart LiquidSoap (works like the time argument to shutdown)" name="time"/>
           </UML:BehavioralFeature.parameter>
          </UML:Operation>
          <UML:Operation visibility="public" isSpecification="false" isQuery="false" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="Gdsibn97f0Kh" comment="Query for the current status of streamer-fallback.&#xa;&#xa;Outputs the current status in JSON format." name="status">
           <UML:BehavioralFeature.parameter>
            <UML:Parameter kind="return" xmi.id="M1o1z8i736UF" type="HJWkeOkEp1og"/>
           </UML:BehavioralFeature.parameter>
          </UML:Operation>
          <UML:Operation visibility="public" isSpecification="false" isQuery="false" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="8wPMCbJ2Sowq" comment="Make any necessary changes to the LiquidSoap configuration file to prepare for a new day.&#xa;&#xa;This also includes downloading the schedule, downloading scheduled reruns and such. Reruns are downloaded even if reruns are not to be played according to status; this is done so that we are not screwed if we later change status to reruns, after systems have gone down.&#xa;&#xa;To be run daily after midnight from a cron job." name="prepare_day">
           <UML:BehavioralFeature.parameter>
            <UML:Parameter kind="return" xmi.id="aMFGCew6kg3w" type="kagvPyRWnTT2"/>
           </UML:BehavioralFeature.parameter>
          </UML:Operation>
          <UML:Operation visibility="public" isSpecification="false" isQuery="false" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="hlFAZB03nmmh" comment="Change the status and/or situation of streamer-fallback.&#xa;&#xa;The user can drop any parameters. Dropped parameters are not touched." name="change">
           <UML:BehavioralFeature.parameter>
            <UML:Parameter kind="return" xmi.id="tFoqfpNHrveb" type="kagvPyRWnTT2"/>
            <UML:Parameter visibility="private" isSpecification="false" xmi.id="GWrL4h1OgpPg" type="R3mptdyUyMiT" value="" comment="New situation to be applied" name="new_situation"/>
            <UML:Parameter visibility="private" isSpecification="false" xmi.id="w8lzCDCtzS3W" type="ioOY0wIqWwRH" value="" comment="New status to apply." name="new_status"/>
            <UML:Parameter visibility="private" isSpecification="false" xmi.id="75VWWnhSDgjf" type="kagvPyRWnTT2" value="" comment="See the live_shows property" name="live_shows_present"/>
            <UML:Parameter visibility="private" isSpecification="false" xmi.id="NFEImyaK2IXL" type="HJWkeOkEp1og" value="" comment="See the nightmusic_show_name property." name="nightmusic_show_name"/>
           </UML:BehavioralFeature.parameter>
          </UML:Operation>
          <UML:Operation visibility="public" isSpecification="false" isQuery="false" isAbstract="false" isLeaf="false" isRoot="false" xmi.id="b1YPmK40otLq" comment="Prepare for several days of downtime.&#xa;&#xa;Usually, the system can tackle multiple hours of downtime (since prepare_day is run daily, and downloads everything that is needed for the next 24 hours). However, using this operation, you can tell core to prepare for multiple days of downtime in advance. By doing so, all reruns scheduled to be run in the given days are downloaded now.&#xa;&#xa;Note that downtime here means downtime for the server which hosts the reruns as well as the API for the schedule, not streamer. streamer can be down all it wants as long as the file server and schedule server is up, since reruns can still be retrieved daily. This is only necessary for the situations where streamer _and_ the file server with the reruns _and_ the server which hosts the schedule API are going to be down for an extended period of time (that means longer than this day).&#xa;&#xa;If the storage required is greater than the storage available, the program ends with error code 2. If the schedule is incomplete, the program ends with error code 3." name="prepare_downtime">
           <UML:BehavioralFeature.parameter>
