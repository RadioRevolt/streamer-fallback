          <UML:Attribute visibility="public" isSpecification="false" xmi.id="fzWC8oky5KyF" type="kagvPyRWnTT2" comment="True if LiquidSoap's config has been changed but LiquidSoap hasn't taken it into use yet, False otherwise." name="liquidsoap_needs_restart"/>
